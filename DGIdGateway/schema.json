{
	"$defs": {
		"timestamp": {"type": "string"},
		"dg-id": {"type": "integer"},
		"action": {"type": "string", "enum": ["linking", "unlinked", "failed"]},
		"reason": {"type": "string", "enum": ["user", "timer", "remote", "startup"]},
		"protocol": {"type": "string", "enum": ["fcs", "ysf", "imrs", "gateway", "parrot", "ysf2dmr", "ysf2nxdn", "ysf2m17"]}
	},

	"status": {
		"type": "object",
		"timestamp": {"$ref": "#/$defs/timestamp"},
		"message": {"type": "string"},
		"required": ["timestamp", "message"]
	},
	
	"link": {
		"type": "object",
		"timestamp": {"$ref": "#/$defs/timestamp"},
		"action": {"$ref": "#/$defs/action"},
		"reason": {"$ref": "#/$defs/reason"},
		"dg-id": {"$ref": "#/defs/dg-id"},
		"protocol": {"$ref": "#/defs/protocol"},
		"descrption": {"type": "string"},
		"required": ["timestamp", "action"]
	}
}

